@model UserViewModel
<div class="form-group">
    <label asp-for="@Model.FName">FName</label>
    <input type="text" asp-for="@Model.FName" class="form-control" readonly />
    <span asp-validation-for="@Model.FName" class="bg-danger"></span>
</div>
<div class="form-group">
    <label asp-for="@Model.LName">Lname</label>
    <input type="text" asp-for="@Model.LName" class="form-control" readonly />
    <span asp-validation-for="@Model.LName" class="bg-danger"></span>
</div>
<div class="form-group">
    <label asp-for="@Model.PhoneNumber">phoneNumber</label>
    <input type="text" asp-for="@Model.PhoneNumber" class="form-control" readonly />
    <span asp-validation-for="@Model.PhoneNumber" class="bg-danger"></span>
</div>
<div class="form-group">
    <label asp-for="@Model.Email">Email</label>
    <input type="text" asp-for="@Model.Email" class="form-control" readonly />
    <span asp-validation-for="@Model.Email" class="bg-danger"></span>
</div>
<div class="form-group">
    <label asp-for="@Model.id">id</label>
    <input type="text" asp-for="@Model.id" class="form-control" readonly />
    <span asp-validation-for="@Model.id" class="bg-danger"></span>
</div>

