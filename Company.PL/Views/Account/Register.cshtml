@model RegisterViewModel ;
@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/_AuthSecurity.cshtml";
}

<div class="main-agileinfo">
	<div class="agileits-top">
		<div asp-validation-summary="All"></div>
		<form asp-action="Register">
			<input class="text" type="text" asp-for="@Model.FName" placeholder="FirstName" required="">
			<input class="text" type="text" asp-for="@Model.LName" placeholder="LastName" required="">
			<input class="text email" type="email" asp-for="@Model.Email" placeholder="Email" required="">
			<input class="text" type="password" asp-for="@Model.Password" placeholder="Password" required="">
			<input class="text w3lpass" type="password" asp-for="@Model.ConfirmPassword" placeholder="Confirm Password" required="">
			<div class="wthree-text">
				<label class="anim">
					<input asp-for="@Model.Agree"  class="checkbox"  required="">
					<span>I Agree To The Terms & Conditions</span>
				</label>
				<div class="clear"> </div>
			</div>
			<input type="submit" value="SIGNUP">
		</form>
		<p>Already have an account <a asp-action="Login"> Login Now!</a></p>
	</div>
</div>
@section scriptValidationSection {
	<partial name="_ValidationScriptsPartial" />

}

