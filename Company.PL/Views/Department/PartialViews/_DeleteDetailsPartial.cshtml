@model DepartmentViewModel
<div asp-validation-summary="All"></div>
<div class="form-group">
    <label asp-for="@Model.Code">Code</label>
    <input type="text" asp-for="@Model.Code" class="form-control" readonly />
    <span asp-validation-for="@Model.Code" class="bg-danger"></span>
</div>
<div class="form-group">
    <label asp-for="@Model.name">Name</label>
    <input type="text" asp-for="@Model.name" class="form-control" readonly />
    <span asp-validation-for="@Model.name" class="bg-danger"></span>
</div>
