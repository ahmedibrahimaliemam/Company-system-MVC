
@model RoleViewModel ;
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Add Roles</h1>
<div asp-validation-summary="All"></div>
<form asp-action="Create" enctype="multipart/form-data">
    <partial name="PartialViews/_EditCreatePartial" model="@Model" />
    <input value="Add" class="bg-info form-control" type="submit" />
</form>
@section scriptValidationSection {
    <partial name="_ValidationScriptsPartial" />

}

